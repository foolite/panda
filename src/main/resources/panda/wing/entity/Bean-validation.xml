<!DOCTYPE validators PUBLIC
		"-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>
	<field name="status">
		<field-validator type="constant">
			<param name="list">consts.dataStatusMap</param>
			<message key="validation-constant"/>
		</field-validator>
		<field-validator type="conversion">
			<message key="validation-conversion-Char"/>
		</field-validator>
	</field>
	<field name="cusid">
		<field-validator type="conversion">
			<message key="validation-conversion-Long"/>
		</field-validator>
	</field>
	<field name="ctime">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
	<field name="uusid">
		<field-validator type="conversion">
			<message key="validation-conversion-Long"/>
		</field-validator>
	</field>
	<field name="utime">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
</validators>
