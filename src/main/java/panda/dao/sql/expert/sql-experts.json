{
	/*
	 * default SqlExpert mapping
	 */
	"experts": {
		"db2.*" : 			"panda.dao.sql.expert.Db2SqlExpert",
		".+derby.+" : 		"panda.dao.sql.expert.DerbySqlExpert",
		"h2.*" : 			"panda.dao.sql.expert.H2SqlExpert",
		"hsql.*" : 			"panda.dao.sql.expert.HsqldbSqlExpert",
		"mysql.*" : 		"panda.dao.sql.expert.MysqlSqlExpert",
		"oracle.*" : 		"panda.dao.sql.expert.OracleSqlExpert",
		"postgresql.*" : 	"panda.dao.sql.expert.PostgreSqlExpert",
		"sqlite" : 			"panda.dao.sql.expert.SQLiteSqlExpert",
		// SqlServer2005 --> 9.0 , SqlServer2008 --> 10.0
		"microsoft sql server.*(8)[.].+" : 		"panda.dao.sql.expert.Mssql2000SqlExpert",
		"microsoft sql server.*(9|10)[.].+" : 	"panda.dao.sql.expert.Mssql2005SqlExpert",
		"microsoft sql server.*(11)[.].+" : 	"panda.dao.sql.expert.Mssql2012SqlExpert"
	},

	/*
	 * global properties
	 */
	"properties": {
		"mysql-engine" : "InnoDB"
	}
}
