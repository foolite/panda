		<dependency org="org.eclipse.jetty" name="jetty-jsp" rev="8.1.19.v20160209" conf="tool->*"/>
		<dependency org="org.eclipse.jetty.orbit" name="org.apache.jasper.glassfish" rev="2.2.2.v201112011158" conf="tool->*"/>
		<dependency org="org.eclipse.jetty.orbit" name="org.apache.taglibs.standard.glassfish" rev="1.2.0.v201112081803" conf="tool->*"/>
		<dependency org="org.eclipse.jetty.orbit" name="org.eclipse.jdt.core" rev="3.7.1" conf="tool->*"/>
		<dependency org="org.eclipse.jetty.orbit" name="javax.el" rev="2.1.0.v201105211819" conf="tool->*"/>
		<dependency org="org.eclipse.jetty.orbit" name="com.sun.el" rev="1.0.0.v201105211818" conf="tool->*"/>
	</dependencies>
</ivy-module>
